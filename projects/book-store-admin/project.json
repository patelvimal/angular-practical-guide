{
    "projectType": "application",
    "schematics": {
        "@schematics/angular:component": {
            "style": "scss"
        },
        "@schematics/angular:application": {
            "strict": true
        }
    },
    "root": "projects/book-store-admin",
    "sourceRoot": "projects/book-store-admin/src",
    "prefix": "app",
    "architect": {
        "build": {
            "builder": "@angular-devkit/build-angular:browser",
            "options": {
                "outputPath": "dist/book-store-admin",
                "index": "projects/book-store-admin/src/index.html",
                "main": "projects/book-store-admin/src/main.ts",
                "polyfills": "projects/book-store-admin/src/polyfills.ts",
                "tsConfig": "projects/book-store-admin/tsconfig.app.json",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "projects/book-store-admin/src/favicon.ico",
                    "projects/book-store-admin/src/assets"
                ],
                "styles": ["projects/book-store-admin/src/styles.scss"],
                "scripts": []
            },
            "configurations": {
                "production": {
                    "budgets": [
                        {
                            "type": "initial",
                            "maximumWarning": "500kb",
                            "maximumError": "1mb"
                        },
                        {
                            "type": "anyComponentStyle",
                            "maximumWarning": "2kb",
                            "maximumError": "4kb"
                        }
                    ],
                    "fileReplacements": [
                        {
                            "replace": "projects/book-store-admin/src/environments/environment.ts",
                            "with": "projects/book-store-admin/src/environments/environment.prod.ts"
                        }
                    ],
                    "outputHashing": "all"
                },
                "development": {
                    "buildOptimizer": false,
                    "optimization": false,
                    "vendorChunk": true,
                    "extractLicenses": false,
                    "sourceMap": true,
                    "namedChunks": true
                }
            },
            "defaultConfiguration": "production"
        },
        "serve": {
            "builder": "@angular-devkit/build-angular:dev-server",
            "configurations": {
                "production": {
                    "browserTarget": "book-store-admin:build:production"
                },
                "development": {
                    "browserTarget": "book-store-admin:build:development"
                }
            },
            "defaultConfiguration": "development"
        },
        "extract-i18n": {
            "builder": "@angular-devkit/build-angular:extract-i18n",
            "options": {
                "browserTarget": "book-store-admin:build"
            }
        },
        "test": {
            "builder": "@angular-devkit/build-angular:karma",
            "options": {
                "main": "projects/book-store-admin/src/test.ts",
                "polyfills": "projects/book-store-admin/src/polyfills.ts",
                "tsConfig": "projects/book-store-admin/tsconfig.spec.json",
                "karmaConfig": "projects/book-store-admin/karma.conf.js",
                "inlineStyleLanguage": "scss",
                "assets": [
                    "projects/book-store-admin/src/favicon.ico",
                    "projects/book-store-admin/src/assets"
                ],
                "styles": ["projects/book-store-admin/src/styles.scss"],
                "scripts": []
            }
        },
        "lint": {
            "builder": "@angular-eslint/builder:lint",
            "options": {
                "lintFilePatterns": [
                    "projects/book-store-admin/**/*.ts",
                    "projects/book-store-admin/**/*.html"
                ]
            }
        }
    }
}
